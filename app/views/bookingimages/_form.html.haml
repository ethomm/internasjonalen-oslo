= form_for @bookingimage do |f|
  - if @bookingimage.errors.any?
    #error_explanation
      %h2= "#{pluralize(@bookingimage.errors.count, "error")} prohibited this bookingimage from being saved:"
      %ul
        - @bookingimage.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :image
    = f.text_field :image
  .field
    = f.label :description
    = f.text_field :description
  .field
    = f.label :fotograf
    = f.text_field :fotograf
  .field
    = f.label :booking_id
    = f.text_field :booking_id
  .actions
    = f.submit 'Save'
